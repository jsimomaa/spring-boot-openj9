    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Application<spring.boot.openj9>",
            "request": "launch",
            "mainClass": "com.example.application.Application",
            "projectName": "spring.boot.openj9",
            "vmArgs": "-Xms256m -Xmx1G -Xshareclasses -Xjit:printErrorInfoOnCompFailure,verbose={compilePerformance},vlog=C:/git/vlog3"
        }
    ]
}